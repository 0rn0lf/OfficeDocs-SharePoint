{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointServer",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "SharePointOnline",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      },
      {
        "files": [
          "**/*.md",
          "**/**.yml"
        ],
        "src": "Hub",
        "dest": ".",
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "globalMetadata": {
      "breadcrumb_path": "/sharepoint/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "searchScope": ["SharePoint Server"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-SharePoint",
      "feedback_product_url": "https://sharepoint.uservoice.com/" 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-SharePoint"
  }
}
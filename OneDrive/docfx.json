{
  "build": {
    "content": [
      {
        "files": [
          "**/*.md",
          "**/*.md"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**",
          "README.md",
          "LICENSE",
          "LICENSE-CODE",
          "ThirdPartyNotices"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "**/*.png",
          "**/*.jpg",
          "**/*.gif",
          "**/*.svg"
        ],
        "exclude": [
          "**/obj/**",
          "**/includes/**"
        ]
      }
    ],
    "overwrite": [],
    "externalReference": [],
    "markdownEngineName": "markdig",    
    "globalMetadata": {
      "breadcrumb_path": "/onedriveforbusiness/breadcrumb/toc.json",
      "extendBreadcrumb": "true",
      "searchScope": ["OneDrive for Business"],
      "feedback_system": "GitHub",
      "feedback_github_repo": "MicrosoftDocs/OfficeDocs-SharePoint",
      "feedback_product_url": "https://onedrive.uservoice.com" 
    },
    "fileMetadata": {},
    "template": [],
    "dest": "OfficeDocs-OneDrive"
  }
}
